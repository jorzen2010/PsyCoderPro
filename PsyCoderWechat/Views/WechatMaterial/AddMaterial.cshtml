 <!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        素材管理
        
    </h1>
</section>
<!-- Main content -->
<section class="content">
    <div class="box box-primary">
        <div class="box-header with-border">
            <h3 class="box-title">Quick Example</h3>
        </div>
        <!-- /.box-header -->
        <!-- form start -->
        <form role="form" method="post" action="/WechatMaterial/UploadMaterial" enctype="multipart/form-data">
            <div class="box-body">
                <div class="form-group">
                    <label for="exampleInputPassword1">素材类型</label>
                    <select class="form-control" name="MaterialType" id="MaterialType">
                        <option value="image">图片(image)</option>
                        <option value="video">视频(video)</option>
                        <option value="thumb">缩略图（thumb）</option>
                        <option value="voice">语音（voice）</option>
                    </select>
                </div>
                <div class="form-group">
                    <label for="exampleInputFile">选择文件</label>
                    <input type="file" name="MaterialFile" id="MaterialFile">
                    <p class="help-block">请选择一个本地文件进行上传.</p>
                </div>

            </div>
            <!-- /.box-body -->
            <div class="box-footer">
                <button type="submit" class="btn btn-primary">立即上传</button>
            </div>
        </form>
    </div>
</section>
<!-- /.content -->